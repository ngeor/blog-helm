{
  "name": "blog-helm",
  "version": "3.2.0",
  "description": "An example hello world application to show Docker and Helm",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "lint-junit": "eslint --quiet -f junit -o test-reports/ci-eslint.xml .",
    "test": "mocha ./test/unit-specs",
    "test-junit": "mocha -R xunit --reporter-options output=test-reports/ci-mocha.xml ./test/unit-specs",
    "start": "node index.js",
    "wdio": "wdio",
    "nyc": "nyc npm test",
    "nyc-junit": "nyc npm run test-junit",
    "start-wdio": "concurrently -k -s first npm:start npm:wdio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngeor/blog-helm.git"
  },
  "author": "Nikolaos Georgiou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngeor/blog-helm/issues"
  },
  "homepage": "https://github.com/ngeor/blog-helm#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@wdio/cli": "^5.21.0",
    "@wdio/junit-reporter": "^5.18.6",
    "@wdio/local-runner": "^5.21.0",
    "@wdio/mocha-framework": "^5.18.7",
    "@wdio/spec-reporter": "^5.18.7",
    "@wdio/sync": "^5.20.1",
    "chai": "^4.2.0",
    "chromedriver": "^80.0.1",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "geckodriver": "^1.19.1",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.1",
    "wdio-chromedriver-service": "^5.0.2",
    "wdio-geckodriver-service": "^1.0.3"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "text",
      "html",
      "teamcity"
    ],
    "exclude": [
      "wdio.conf.js",
      "ci-scripts/*.js",
      "coverage/**/*.js",
      "test/**/*.js"
    ]
  }
}
